{
    "source": {
      "include": ["src"],
      "includePattern": ".js$",
      "exclude": ["node_modules"]
    },
    "plugins": ["plugins/markdown","node_modules/better-docs/category"],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false,
      "default": {
        "outputSourceFiles": true
      }
    },
    "opts": {
      "template": "node_modules/better-docs",
      "destination": "docs/",
      "recurse": true
    },
    "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc"]
    }
  }
  